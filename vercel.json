{
    "builds": [{
            "src": "package.json",
            "use": "@vercel/node"
        },
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*.test.{ts,tsx}",
            "use": "@vercel/static-build"
        }
    ]
}